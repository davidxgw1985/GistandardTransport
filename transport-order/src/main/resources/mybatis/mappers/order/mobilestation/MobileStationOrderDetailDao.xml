	<select id="queryMobileStationOrderList" parameterType="com.gistandard.transport.order.module.mobilestation.bean.MoStationOrderDbQueryReq" resultMap="result-mobileDetail">
		select
		<include refid="Agency_Order_List" />
		from MOBILE_BOOKING_FORM mbf
		LEFT JOIN BOOKING_FORM b on b.ID = mbf.BOOKING_FORM_ID
		<where>
			mbf.DESTN_LOCUS = 'POD'
			<if test="mobileStationFormId != null">
				and mbf.ID = #{mobileStationFormId}
			</if>
			<if test="busiCtrl != null">
				and mbf.BUSI_CTRL = #{busiCtrl}
			</if>
			<if test="productTypeList != null">
				AND mbf.PRODUCT_TYPE IN
				<foreach collection="productTypeList" index="index" item="item" open="(" separator="," close=")">
					#{item}
				</foreach>
			</if>
			<if test="shipperName != null">
				and mbf.SHIP_CUST_LINK_MAN like CONCAT('%', #{shipperName}, '%')
			</if>
			<if test="receiverName != null">
				and mbf.CNEE_CUST_LINK_MAN like CONCAT('%', #{receiverName}, '%')
			</if>
			<if test="startTime != null">
				and mbf.CREATE_DATE >= #{startTime}
			</if>
			<if test="endTime != null">
				<![CDATA[and mbf.CREATE_DATE <= #{endTime}]]>
			</if>
			<if test="createUserId != null">
				and mbf.CREATE_USER_ID = #{createUserId}
			</if>
			<if test="revUserId!=null">
				and mbf.REV_USER_ID = #{revUserId}
			</if>
			<if test="roleId!=null">
				and mbf.ROLE_ID = #{roleId}
			</if>
			<if test="areaId!=null">
				<!-- 查询区的时候 -->
				<if test="areaId&gt;3999">
					AND mbf.CNEE_CUST_COUNTY = #{areaId}
				</if>
				<!-- 查询市的时候 -->
				<if test="areaId&lt;4000">
					AND mbf.CNEE_CUST_CITY = #{areaId}
				</if>
			</if>
		</where>
		ORDER BY mbf.CREATE_DATE DESC
		<if test="startRecord >= 0 and pageSize > 0">
			limit #{startRecord},#{pageSize}
		</if>
	</select>

